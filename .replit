modules = ["nodejs-20", "web"]
[nix]
channel = "stable-25_05"

[deployment]
build = ["npm", "run", "build"]
deploymentTarget = "static"
publicDir = "dist"

[[deployment.rewrites]]
from = "/*"
to = "/index.html"

[workflows]
runButton = "Development Server"

[[workflows.workflow]]
name = "Development Server"
author = 32687169
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"

[agent]
expertMode = true
